<mvc:View xmlns:uxap="sap.uxap" xmlns:actiontoolbar="sap.ui.mdc.actiontoolbar" xmlns:mdc="sap.ui.mdc"
    controllerName="zeiterfassung.controller.App"
    xmlns:f="sap.f"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <f:DynamicPage>
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title text="{i18n>title}" />
                </f:heading>
                <f:actions>
                    <Button
                        type="Emphasized"
                        icon="sap-icon://add"
                        text="Create Entry" />
                </f:actions>
            </f:DynamicPageTitle>
        </f:title>
        <f:content>
            <App id="app" />
        </f:content>
    </f:DynamicPage>
</mvc:View>
