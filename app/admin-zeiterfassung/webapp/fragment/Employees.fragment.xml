<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        title="Employees"
        items="{/Employees}"
        multiSelect="true"
        confirm=".onPressConfirmEmployeesDialog"
        cancel=".onPressCancelEmployeesDialog">
        <StandardListItem
            title="{firstname} {lastname}" />
    </SelectDialog>
</core:FragmentDefinition>